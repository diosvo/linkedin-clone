    "eslint-plugin-prefer-arrow": "1.2.3",
    "jest": "^27.4.7",
    "jest-preset-angular": "^11.0.1",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "typescript": "~4.5.4"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-config.ts"
